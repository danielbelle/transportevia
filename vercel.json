{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.3" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/build/assets/(.*)",
            "dest": "/public/build/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/vendor/sign-pad/(.*)",
            "dest": "/public/vendor/sign-pad/$1"
        }
    ],
    "outputDirectory": "public",
    "env": {
        "APP_URL": "https://transportevia.vercel.app/",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "PHP_INI_SCAN_DIR": "/var/task/user/php/conf.d:/var/task/php/conf.d",
        "PHP_EXTENSION_CURL": "1",
        "PHP_EXTENSION_PGSQL": "1",
        "PHP_EXTENSION_PDO_PGSQL": "1"
    }
}
